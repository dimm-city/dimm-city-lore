---
import ContentPane from "../components/ContentPane.astro";
let title = Astro.props.title;
---

<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta
      name="viewport"
      content="width=device-width, height=device-height, initial-scale=1.0, user-scalable=false"
    />
    <title>{title} - Dimm City</title>
    <link rel="icon" type="image/x-icon" href="/assets/icons/acorn256.png" />
    <style global>
      @import "../styles/print.css";
    </style>
    <slot name="styles" />
  </head>
  <body>
    <slot>404</slot>
  </body>
</html>
